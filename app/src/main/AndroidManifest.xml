<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxzx74147.charttest">


    <application
        android:name=".ZXApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="api_version"
            android:value="1.0.0">


        </meta-data>

        <meta-data
            android:name="com.alibaba.app.appkey"
            android:value="24814378" /> <!-- 请填写你自己的- appKey -->
        <meta-data
            android:name="com.alibaba.app.appsecret"
            android:value="b4474e6cf05380d7f6d7c380edbbb9e8" /> <!-- 请填写你自己的appSecret -->

        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5a9668fba40fa3717e00025a" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="main" />

        <activity android:name=".MainActivity"></activity>

        <activity
            android:name=".MainFeedActivity"
            android:launchMode="singleInstance"></activity>


        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.zxzx74147.live.layout.LiveActivity"
            android:screenOrientation="portrait" />

        <receiver android:name=".push.AliPushReceiver">
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_OPENED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.push2.action.NOTIFICATION_REMOVED" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.alibaba.sdk.android.push.RECEIVE" />
            </intent-filter>
        </receiver>
    </application>

</manifest>