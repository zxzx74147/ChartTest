    <style name="content_black">
        <item name="android:textSize">@dimen/default_size_30</item>
        <item name="android:textColor">@color/text_black</item>
    </style>


    <style name="CircularProgress" parent="Theme.AppCompat.Light">
        <!--<item name="colorBackground">@color/progress_bg</item>-->
